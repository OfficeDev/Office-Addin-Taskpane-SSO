{
  "name": "office-addin-taskpane-sso",
  "version": "0.0.0",
  "private": true,
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "build:dev": "webpack --mode development --https false",
    "configure-sso": "office-addin-sso configure manifest.xml",
    "convert-to-single-host": "node convertToSingleHost.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "sideload": "office-addin-debugging start manifest.xml",
    "start": "npm run build:dev && concurrently \"npm run start:server\" \"npm run sideload\"",
    "start:server": "office-addin-sso start manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --watch --mode development"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "msal": "^1.1.3",
    "node-fetch": "^2.6.0",
    "office-addin-sso": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.12.5",
    "@types/office-js": "^1.0.50",
    "@types/office-runtime": "^1.0.9",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint-config-office-addins": "^1.0.11",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "office-addin-cli": "^1.0.4",
    "office-addin-debugging": "^3.0.16",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-lint": "^1.0.13",
    "office-addin-manifest": "^1.4.13",
    "office-addin-prettier-config": "^1.0.11",
    "concurrently": "^4.1.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
}
